class Point {
   double x;
   double y; 
   Point origin = new x(0,0); 
   Point x(double x_value, double y_value) {
      x = x_value; 
      y = y_value; 
   }
   void clear() {
      this.x(0); 
      this.y(0); 
   }
   double distance(Point that) {
      double xDiff = x - that.x(); 
      double yDiff = y - that.y(); 
      return Math.sqrt(xDiff * xDiff + yDiff * yDiff);
   }
}