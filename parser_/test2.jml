class OddEven
{
    int input;

    void main(String[] args)
    {
        OddEven number = new OddEven();
        number.showDialog();
    }
 
    void showDialog()
    {


		input = Integer.parseInt(JOptionPane.showInputDialog("Please Enter A Number"));
		calculate();
        }

    }
 

    void calculate()
    {
        if ((input / 2) == 0)
        {
            JOptionPane.showMessageDialog(null, "Even");
        }
        else
        {
            JOptionPane.showMessageDialog(null, "Odd");
        }
    }
}